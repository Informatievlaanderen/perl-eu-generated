  "classes": [
    {
      "uri": "https://purl.eu/ns/air-and-water/water#BioIndicatorObservation",
      "name": "BioIndicatorObservation",
      "label": {
        "en": "BioIndicatorObservation"
      },
      "sort": "BioIndicatorObservation",
      "description": {
        "en": "Observation that assesses the biological Quality of Water based on the overall condition of indicator species."
      },
      "usage": {
        "en": "These species, also called BioIndicators, are living organisms such as plants, planktons, animals, microbes etc. More specifically, biochemical, physiological, behavioural or population changes are monitored."
      },
      "equivalent": [],
      "parents": [
        "http://def.isotc211.org/iso19156/2011/Observation#OM_Observation"
      ]
    },
    {
      "uri": "https://purl.eu/ns/air-and-water/water#BioticIndexObservation",
      "name": "BioticIndexObservation",
      "label": {
        "en": "BioticIndexObservation"
      },
      "sort": "BioticIndexObservation",
      "description": {
        "en": "Observation that assesses the biological Quality of Water on a certain scale based on what organisms are present in the Water and in what quantities."
      },
      "usage": {
        "en": "In practice this comes down to counting specimens of certain species of macro-invertebrates like worms, snails, flies, clams etc present in the Water, weighted by the extent to which they tolerate Water pollution and combined into a single WaterqQuality score."
      },
      "equivalent": [],
      "parents": [
        "http://def.isotc211.org/iso19156/2011/Observation#OM_Observation",
        "https://def.isotc211.org/iso19156/2011/Observation#Measurement"
      ]
    },
    {
      "uri": "https://purl.eu/ns/air-and-water/water#ChemicalAgentConcentrationObservation",
      "name": "ChemicalAgentConcentrationObservation",
      "label": {
        "en": "ChemicalAgentConcentrationObservation"
      },
      "sort": "ChemicalAgentConcentrationObservation",
      "description": {
        "en": "Observation to assess  the chemical Quality of Water by estimating the concentration of a harmful or unsuitable substance."
      },
      "usage": {
        "en": "Actually the Measurement of the amount of the substance in the Water, usually over some averaging period. Result is how much of the substance is found eg in milligrams-per-liter."
      },
      "equivalent": [],
      "parents": [
        "http://def.isotc211.org/iso19156/2011/Observation#OM_Observation",
        "https://def.isotc211.org/iso19156/2011/Observation#Measurement"
      ]
    },
    {
      "uri": "https://purl.eu/ns/air-and-water/water#WaterFeature",
      "name": "WaterFeature",
      "label": {
        "en": "WaterFeature"
      },
      "sort": "WaterFeature",
      "description": {
        "en": "Real-world Object related to Water."
      },
      "usage": {
        "en": "It is the featureOfInterest, the Object of which some property is observed to assess WaterQuality. This can be for example a waterbody like the Canal Brussels-Charleroi. The Waterfeature can be typed via the \"type\" attribute (value would be \"waterbody-canal\" for example), but substitution by a typed instance (eg an instance of type \"Canal\", subclass of a class \"Waterbody\") is also allowed. In either case, the observedProperty must be a phenomenon related to the WaterFeature type. If the WaterFeature is not observed directly, but rather indirectly by sampling it, the WaterFeature is the sampledObject and the sample assumes the role of featureOfInterest."
      },
      "equivalent": [],
      "parents": [
        "https://def.isotc211.org/iso19156/2011/GeneralFeatureInstance#GFI_Feature"
      ]
    },
    {
      "uri": "https://purl.eu/ns/air-and-water/water#WaterQualityObservationCollection",
      "name": "WaterQualityObservationCollection",
      "label": {
        "en": "WaterQualityObservationCollection"
      },
      "sort": "WaterQualityObservationCollection",
      "description": {
        "en": "A Collection of one or more WaterQualityObservations,whose members share a common value for one or more properties."
      },
      "usage": {},
      "equivalent": [],
      "parents": [
        "https://www.w3.org/TR/vocab-ssn-ext/#sosa:ObservationCollection"
      ]
    },
    {
      "uri": "https://purl.eu/ns/air-and-water/water#WaterQualityParameterObservation",
      "name": "WaterQualityParameterObservation",
      "label": {
        "en": "WaterQualityParameterObservation"
      },
      "sort": "WaterQualityParameterObservation",
      "description": {
        "en": "Observation to assess physical Quality of Water by measuring certain physical quantities."
      },
      "usage": {
        "en": "These quantities are parameters like temperature, salinity, turbidity, suspended/disolved solids etc."
      },
      "equivalent": [],
      "parents": [
        "http://def.isotc211.org/iso19156/2011/Observation#OM_Observation",
        "https://def.isotc211.org/iso19156/2011/Observation#Measurement"
      ]
    }
